@model Ddi.Registry.Data.HttpResolver

@{
    ViewData["Title"] = "Delete " + Model.ResolutionType;
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Delete Http Resolver</h2>

<h3>Are you sure you want to delete this resolution?</h3>
<fieldset>
	<div class="display-label">Resolution Type</div>
    <div class="display-field">@Model.ResolutionType</div>

    <div class="display-label">Url Template</div>
    <div class="display-field">@Model.UrlTemplate</div>
</fieldset>
@using (Html.BeginForm()) {
    <p>
		@Html.HiddenFor(s => s.Id)
        <input type="submit" value="Delete" /> |
        @Html.ActionLink("Back to Assignment Page", "EditAssignment", new { assignmentId = Model.AssignmentId })
    </p>
}


