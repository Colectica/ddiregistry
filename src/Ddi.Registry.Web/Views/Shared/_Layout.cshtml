<!DOCTYPE html>
<html>
<head>
@if (!string.IsNullOrWhiteSpace(ViewBag.Title))
{
	<title>@ViewBag.Title - DDI Registry</title>
}
else
{
	<title>DDI Registry</title>
}
	
    
<link type="text/css" rel="stylesheet" href="//netdna.bootstrapcdn.com/bootswatch/3.0.2/cerulean/bootstrap.min.css" media="all" />
<style>
@@import url("/Content/overrides.css");
</style>
<style>
@@import url("/Content/font.css");
</style>

    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />	
    <script src="@Url.Content("~/Scripts/jquery-1.4.4.min.js")" type="text/javascript"></script>
    <script src='https://www.google.com/recaptcha/api.js'></script>
</head>

<body class="html front not-logged-in no-sidebars page-node page-node- page-node-77 node-type-page navbar-is-static-top" >

  
<header id="navbar" role="banner" class="navbar navbar-static-top navbar-inverse">
  <div class="container">
    <div class="navbar-header">
            <a class="logo navbar-btn pull-left" href="/" title="Home"><img src="/Content/img/logo.png" alt="Home" /></a>    

       <!-- .btn-navbar is used as the toggle for collapsed navbar content -->
      <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>
      </div>
      

    <div class="collapse navbar-collapse">
      <ul class="nav navbar-nav">
							<li>@Html.ActionLink("Home", "Index", "Home")</li>
							<li>@Html.ActionLink("Manage", "Index", "Manage")</li>
							<li>@Html.ActionLink("Tools", "Tools", "Home")</li>
							<li>@Html.ActionLink("Help", "Index", "Help")</li>
      </ul>
      
      <ul class="nav navbar-nav navbar-right">
        @Html.Partial("_LogOnPartial")
      </ul>
    </div><!-- /.navbar-collapse -->
  </div><!-- /.container-fluid -->
</header>


<div class="main-container container">

  <header role="banner" id="page-header">
    
      </header> <!-- /#page-header -->

  <div class="row">

    
    <section class="col-sm-12">
                  <a id="main-content"></a>
                                                               
       
       
<div class="row">
<div class="col-md-12">	
@RenderBody()    
</div>
</div>       
       
       
       
       
       
       



  		
      
    </section>

    
  </div>
</div>
<footer class="footer container">
    <div class="region region-footer well well-lg">
    <section id="block-block-15" class="block block-block clearfix">

      
  <div class="row">

	<div class="col-md-3">Have questions? <br /><a href="mailto:ddiregistry@umich.edu"><span class="glyphicon glyphicon-envelope"></span> Contact the DDI Registry team</a></div>

	<div class="col-md-6">&copy; @DateTime.Now.Year DDI Alliance</div>

	<div class="col-md-3">&nbsp;</div>
</div>

</section> <!-- /.block -->
  </div>
</footer>
</body>
</html>


</html>
