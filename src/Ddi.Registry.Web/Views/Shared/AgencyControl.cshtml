@model Ddi.Registry.Data.Agency

<dl class="row">
   
    <dt class="col-sm-3">Agency Name:</dt>
    <dd class="col-sm-9">@Html.DisplayFor(model => model.AgencyId)</dd>

    <dt class="col-sm-3">Agency Label:</dt>
    <dd class="col-sm-9">@Html.DisplayFor(model => model.Label)</dd>

    <dt class="col-sm-3">Date Created:</dt>
    <dd class="col-sm-9">@Html.DisplayFor(model => model.DateCreated)</dd>

    <dt class="col-sm-3">Last Modified:</dt>
    <dd class="col-sm-9">@Html.DisplayFor(model => model.LastModified)</dd>

    <dt class="col-sm-3">Date Approved:</dt>
    <dd class="col-sm-9">@Html.DisplayFor(model => model.DateApproved)</dd>
  
</dl>