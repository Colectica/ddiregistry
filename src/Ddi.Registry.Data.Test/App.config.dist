<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <connectionStrings>
    <clear/>
    <!-- Modify username and password for deployment -->
    <add name="DdiRegistry" 
         connectionString="Server=localhost;Port=5432;Database=ddi_registry;User Id=postgres;Password=yourpassword;Encoding=UNICODE;Sslmode=Prefer;Pooling=true;"
         providerName="Npgsql"/>
  </connectionStrings>
  
  <system.data>
    <DbProviderFactories>
      <remove invariant="Npgsql"/>
      <add name="Npgsql Data Provider" invariant="Npgsql" support="FF"
           description=".Net Framework Data Provider for Postgresql Server"
           type="Npgsql.NpgsqlFactory, Npgsql, Version=2.0.11.0, Culture=neutral, PublicKeyToken=5d8b90d52f46fda7">
      </add>
    </DbProviderFactories>
  </system.data>

</configuration>